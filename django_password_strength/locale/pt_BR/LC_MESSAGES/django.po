msgstr "Atenção:"

#: django_password_strength/templates/django_password_strength/widgets/progressbar-info.html:8
msgid "This password would take <em class=\"password_strength_time\"></em> to crack."
msgstr "Essa senha levaria <em class=\"password_strength_time\"></em> para ser descoberta."

#: django_password_strength/templates/django_password_strength/widgets/strength-info.html:8
msgid "Your passwords don't match."
msgstr "As duas senhas não combinam."

#: django_password_strength/validators.py:13
#, python-format
msgid "Must have at least %(limit_value)d character (it has %(show_value)d)."
msgid_plural "Must have at least %(limit_value)d characters (it has %(show_value)d)."
msgstr[0] "Deve ter pelo menos %(limit_value)d letra (tem %(show_value)d)."
msgstr[1] "Deve ter pelo menos %(limit_value)d letras (tem %(show_value)d)."

#: django_password_strength/validators.py:30
msgid "Must have at least minLength characters long"
msgstr "Deve ter pelo menos minLength letra(s)"

#: django_password_strength/validators.py:36
#, python-format
msgid "Must have at least %(limit_value)d special character (it has %(show_value)d)."
msgid_plural "Must have at least %(limit_value)d special characters (it has %(show_value)d)."
msgstr[0] "Deve ter pelo menos %(limit_value)d letra especial (tem %(show_value)d)."
msgstr[1] "Deve ter pelo menos %(limit_value)d letras especiais (tem %(show_value)d)."

#: django_password_strength/validators.py:53
msgid "Must have at least minLength special character"
msgstr "Deve ter pelo menos minLength letra(s) especial"

#: django_password_strength/validators.py:61
#, python-format
msgid "Must have at least %(limit_value)d lower case character (it has %(show_value)d)."
